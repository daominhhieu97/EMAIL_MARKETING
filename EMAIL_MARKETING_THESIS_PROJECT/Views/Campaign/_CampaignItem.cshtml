@model EMAIL_MARKETING_THESIS_PROJECT.Models.Campaigns.Campaign

<tr>
	<td>@Model.Title</td>
	<td>@Model.Scheduler.SendOn</td>
	<td>@Model.EmailInfo.Subject</td>
	<td>
		<a asp-controller="Campaign" asp-action="Details" asp-route-id="@Model.Id" class="btn btn-info"><i class="fa fa-info-circle" aria-hidden="true"></i></a>
		<a asp-controller="Campaign" asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-info"><i class="fa fa-trash" aria-hidden="true"></i></a>
	</td>
</tr>